    "json-cursor",
    "json-parser",
    "json-path",
    "parser"
  ],
  "author": "7PH <b.raymond@protonmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rollup": "^4.18.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "size-limit": "^11.1.4",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  },
  "size-limit": [
    {
      "limit": "900 B",
      "path": "dist/*.min.js"
